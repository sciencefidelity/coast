---
import Navbar from "components/Navbar.astro"
import { Navigation, Settings } from "lib/interfaces"

const { navigation, settings } = Astro.props as {
  navigation: Navigation
  settings: Settings
}
---
<header id="header" class="relative z-50">
  <Navbar
    navigation={navigation}
    settings={settings}
  />
</header>

<style>
  .hero {
    height: 340px;
  }
</style>
